{
  "name": "eslint-config-onny",
  "version": "1.0.7",
  "description": "Lint specs for Onny",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js --ext .jsx --ext .json --format ./eslintFormat",
    "lint-fix": "eslint . --ext .js --ext .jsx --ext .json --format ./eslintFormat --fix",
    "lint-fix_test": "eslint ./test --ext .js --format ./eslintFormat --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Onnyio/eslint-config-onny.git"
  },
  "author": "Isaac Marotte <isaac@onny.io> (http://onny.io)",
