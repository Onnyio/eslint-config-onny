    "Isaac Marotte <isaac@onny.io> (http://onny.io)"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/onnyio/eslint-config-onny/issues"
  },
  "homepage": "https://github.com/onnyio/eslint-config-onny#readme",
  "dependencies": {
    "@babel/core": "^7.12.13",
    "@babel/eslint-parser": "^7.13.14",
    "chalk": "^4.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
